package com.adiro.nasm_ide;

import javafx.scene.layout.HBox;
import javafx.scene.layout.VBox;
import javafx.scene.text.Text;

public class RegistersView extends VBox{

	public RegistersView() {
		var ax = new HBox(new Text("AX: "), new Text("255"));
		var bx = new HBox(new Text("BX: "), new Text("255"));
		var cx = new HBox(new Text("CX: "), new Text("255"));
		var dx = new HBox(new Text("DX: "), new Text("255"));
	}
}
