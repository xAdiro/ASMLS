package com.adiro.nasm_ide.gui.button;

import com.adiro.nasm_ide.gui.content.SourceCodeView;
import com.adiro.nasm_ide.logic.LogReader;

import javafx.event.ActionEvent;
import javafx.event.EventHandler;
import javafx.scene.control.Button;

public class MoveNextButton extends Button{
	public MoveNextButton(SourceCodeView sourceArea, LogReader reader) {
		super(">");
		this.setOnAction(new EventHandler<ActionEvent> () {
			@Override public void handle(ActionEvent e) {
				moveNext(sourceArea, reader);
			}
		});
	
	}
	

	private void moveNext(SourceCodeView sourceArea, LogReader reader) {
		
			
			reader.NextLine();
		
		
	}

}
